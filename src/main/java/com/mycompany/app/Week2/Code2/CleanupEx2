package com.mycompany.app.Week2.Code2.CleanupEx2;

public class Ex2 {
    class StringID {
        private String iStringD = "Initialized at definition";

        public String getIStringD() {
            return iStringD;
        }
    }

    class StringIC {
        private String iStringC;

        public StringIC(String iStringC) {
            this.iStringC = iStringC;
        }

        public String getIStringC() {
            return iStringC;
        }
    }
    // 1 cai khoi tao luon
    // 1 cai goi ra tu constructor roi moi khoi tao gia tri
}
